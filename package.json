{
    "name": "express",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon --watch src -e ts --exec \"npm run lint && npm run build && node dist/main.js\"",
        "build": "rm -rf dist && swc src -d dist --strip-leading-paths",
        "clean": "rm -rf ./dist",
        "lint": "eslint \"src/**/*.ts\"",
        "format": "prettier --write .",
        "api-docs": "swagger-cli bundle docs/openapi.yaml --outfile ./docs/swagger.yaml --type yaml --verbose"
    },
    "dependencies": {
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "express-validator": "^7.2.0",
        "helmet": "^7.1.0",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.14.2",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@swc/cli": "^0.4.0",
        "@swc/core": "^1.7.22",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/helmet": "^4.0.0",
        "@types/node": "^22.5.1",
        "@types/swagger-ui-express": "^4.1.6",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.9.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "prettier-plugin-prisma": "^5.0.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.3.0"
    }
}
